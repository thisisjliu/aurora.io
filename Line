class Line {
  int initialY1;
  int x1;
  int y1;
  int x2;
  int y2;
  int range;

  float hue;
  int sat;
  int bri;
  float alpha;
  
  public Line(int x1, int y1, int x2, int y2, float hue, int sat, int bri, float alpha, int range) {
    this.initialY1 = y1;
    
    this.x1 = x1;
    this.y1 = y1;
    this.x2 = x2;
    this.y2 = y2;
    this.hue = hue;
    this.sat = sat;
    this.bri = bri;
    this.alpha = alpha;
    this.range = range;
  }
}
